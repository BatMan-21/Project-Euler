#include<bits/stdc++.h>
#define ll          long long
#define pb          push_back
#define mp          make_pair
#define pii         pair<int,int>
#define vi          vector<int>
#define all(a)      (a).begin(),(a).end()
#define sz(x)       (int)x.size()
#define hell        1000000007
#define lbnd        lower_bound
#define ubnd        upper_bound
#define bs          binary_search
using namespace std;

int main()
{
	ll t,n;
	cin>>t;
	ll ans1,ans2;
	while(t--)
	{
		cin>>n;
		ans1=n*(n+1)/2;         // sum of first n natural numbers.
		ans2=(n*(n+1))*(2*n+1)/6;   // sum of squares of first n natural numbers.
		ans1=ans1*ans1;
		cout<<abs(ans1-ans2)<<endl;
	}
}
